<% console %>
<div class="container">
    <% flash.each do |name, msg| %>
        <div class="alert alert<%= name == 'notice' ? 'success' : 'error' %> alert-dismissible fade show" role="alert">
            <%= msg %>
            <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
        </div>
    <% end %>
</div>

